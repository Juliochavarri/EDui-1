{
	"name": "@edteam/edui",
	"version": "1.1.1-beta",
	"description": "Libreria de componentes React siguiendo la guia de estilos de EDteam",
	"main": "dist/index.js",
	"license": "MIT",
	"author": "EDteam <contacto@ed.team>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edteamlat/EDui.git"
	},
	"keywords": [
		"React",
		"Styled components",
		"EDteam"
	],
	"bugs": {
		"url": "https://github.com/edteamlat/EDui/issues"
	},
	"homepage": "https://github.com/edteamlat/EDui#edui",
	"scripts": {
		"dev": "rollup --config rollup.dev.js --watch",
		"test": "jest --watch",
		"build": "rollup --config rollup.prod.js",
		"lint": "eslint src --cache",
		"quality": "npm run lint && npm run test",
		"storybook": "start-storybook",
		"publish-library": "npm run build && npm publish",
		"edteam": "npm publish --access public"
	},
	"devDependencies": {
		"@babel/core": "^7.5.0",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@storybook/addon-actions": "^5.1.9",
		"@storybook/addon-info": "^5.1.9",
		"@storybook/addon-notes": "^5.1.9",
		"@storybook/addon-viewport": "^5.1.9",
		"@storybook/react": "^5.1.9",
		"babel-eslint": "^10.0.2",
		"babel-jest": "^24.8.0",
		"babel-loader": "^8.0.6",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-jest": "^22.6.4",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.13.0",
		"jest": "^24.8.0",
		"jest-svg-transformer": "^1.0.0",
		"prettier": "1.18.2",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"rollup": "^1.15.5",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-node-resolve": "^5.0.2",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-svg": "^2.0.0",
		"rollup-plugin-uglify": "^6.0.2",
		"rollup-watch": "^4.3.1",
		"styled-components": "^4.3.1",
		"uglify-es": "^3.3.9"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"react": "^16.6.3",
		"styled-components": ">= 4"
	},
	"dependencies": {
		"polished": "^3.4.1",
		"prop-types": "^15.7.2"
	},
	"jest": {
		"setupFilesAfterEnv": [
			"<rootDir>/jest.setup.js"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/.next/",
			"<rootDir>/dist/"
		],
		"transform": {
			".*": "babel-jest",
			"^.+\\.js?$": "babel-jest",
			"^.+\\.svg$": "jest-svg-transformer"
		}
	}
}
